{
  "manifest_version": 2,
  "name": "Sneekr",
  "short_name": "Sneekr",
  "description": "Sneekr extension collects your browser history and stores it in our dashboard",
  "homepage_url": "https://sneekr.herokuapp.com/",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "build/background-bundle.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "web_accessible_resources": [
    "build/*"
  ],
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
